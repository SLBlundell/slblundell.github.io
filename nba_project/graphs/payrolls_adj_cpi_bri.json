{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "NBA Franchise Adjusted_Payrolls",
    "subtitle": [
      "Active Payroll per NBA franchise, adjusted for inflation and NBA League Revenues. CPI: 1982=0",
      "Source: Spotrac, Forbes, U.S. Bureau of Labor Statistics"
    ],
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "width": 500,
  "height": 500,
  "encoding": {
    "x": {"field": "Code", "type": "nominal", "title": null},
    "y": {
      "field": "Adjusted_Payroll",
      "type": "quantitative",
      "title": null,
      "axis": {"format": "$,.0f"},
      "scale": {"domain": [0, 200000]}
    },
    "color": {"value": "#DF9D9D"},
    "tooltip": [
      {"field": "Team", "title": "Team"},
      {
        "field": "Adjusted_Payroll",
        "title": "Active Payroll Adjusted",
        "format": "$,"
      }
    ]
  },
  "params": [
    {
      "name": "selectedDate",
      "value": 2022,
      "bind": {
        "input": "range",
        "min": 2011,
        "max": 2022,
        "step": 1,
        "name": "Select Year:"
      }
    }
  ],
